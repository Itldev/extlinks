<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
<beans>
    <!-- Registration of new models -->
    <bean id="fr.itldev.extLinks.dictionaryBootstrap" parent="dictionaryModelBootstrap"
          depends-on="dictionaryBootstrap">
        <property name="models">
            <list>
                <value>alfresco/module/${artifactId}/model/extendedLinksModel.xml</value>
            </list>
        </property>
        <property name="labels">
            <list>
                <value>alfresco/module/${artifactId}/model/extendedLinksModel</value>
            </list>
        </property>    
    </bean>
    
    
</beans>
